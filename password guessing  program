
secret_number = 74489
count = 0

while True:

    if count < 3:
        guess = int(input("Enter password : "))
    
        if guess == secret_number:
            print("You got it ...")
            break
        elif guess != secret_number and count != 2:
            print("Try again....")
    
        else:
            print("oops! you used all chances. Try again..")
            break
    count+=1
