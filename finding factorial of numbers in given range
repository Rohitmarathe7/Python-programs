print("This program gives factorial of number's in given range")
while True:

    initial_number = int(input("Enter a starting number : "))
    final_number = int(input("Enter a final input : "))

    for i in range(initial_number, final_number + 1):
        fact = 1
        for j in range(1, i + 1):
            fact = fact * j
        print(f"fact of {i} is  { fact}")
